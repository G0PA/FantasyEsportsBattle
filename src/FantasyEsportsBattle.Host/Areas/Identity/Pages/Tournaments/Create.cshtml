@page
@using FantasyEsportsBattle.Host.Areas.Identity.Pages.Tournaments
@model CreateModel
@{
    ViewData["Title"] = "Tournaments";
}

<h4>@ViewData["Title"]</h4>
<div class="row">
    <div class="col-md-6">
        <h1>Available Competitions:</h1>
        
        <form method="post">
            @foreach (var c in Model.Competitions)
            {
                <checkbox>@c</checkbox>
            }
        </form>
    </div>
</div>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}